<?xml version="1.0" encoding="UTF-8"?><project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <concurrentBuild>false</concurrentBuild>
  <canRoam>true</canRoam>
  <scm class="hudson.plugins.git.GitSCM">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <name>origin</name>
        <refspec>+refs/heads/*:refs/remotes/origin/*</refspec>
        <url>file:///var/lib/jenkins/jobs/iTrust_Build/workspace/iTrust2-v2</url>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>origin/fuzzer</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <disableSubmodules>false</disableSubmodules>
    <recursiveSubmodules>false</recursiveSubmodules>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <remotePoll>false</remotePoll>
    <gitTool>Default</gitTool>
    <submoduleCfg class="list"/>
    <reference/>
    <gitConfigName/>
    <gitConfigEmail/>
    <extensions>
      <hudson.plugins.git.extensions.impl.WipeWorkspace/>
    </extensions>
  </scm>
  <builders>
    <hudson.tasks.Shell>
      <command>cd iTrust2
mvn test || exit 0
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.jacoco.JacocoPublisher plugin="jacoco">
      <execPattern>**/**.exec</execPattern>
      <classPattern>**/classes</classPattern>
      <sourcePattern>**/src/main/java</sourcePattern>
      <changeBuildStatus>false</changeBuildStatus>
      <inclusionPattern/>
      <exclusionPattern/>
      <maximumInstructionCoverage>0</maximumInstructionCoverage>
      <minimumInstructionCoverage>0</minimumInstructionCoverage>
      <maximumBranchCoverage>0</maximumBranchCoverage>
      <minimumBranchCoverage>0</minimumBranchCoverage>
      <maximumComplexityCoverage>0</maximumComplexityCoverage>
      <minimumComplexityCoverage>0</minimumComplexityCoverage>
      <maximumLineCoverage>0</maximumLineCoverage>
      <minimumLineCoverage>0</minimumLineCoverage>
      <maximumMethodCoverage>0</maximumMethodCoverage>
      <minimumMethodCoverage>0</minimumMethodCoverage>
      <maximumClassCoverage>0</maximumClassCoverage>
      <minimumClassCoverage>0</minimumClassCoverage>
    </hudson.plugins.jacoco.JacocoPublisher>
    <hudson.tasks.junit.JUnitResultArchiver plugin="junit">
      <testResults>**/target/surefire-reports/*.xml</testResults>
      <keepLongStdio>false</keepLongStdio>
      <healthScaleFactor>1.0</healthScaleFactor>
      <allowEmptyResults>false</allowEmptyResults>
      <testDataPublishers>
        <de.esailors.jenkins.teststability.StabilityTestDataPublisher/>
      </testDataPublishers>
    </hudson.tasks.junit.JUnitResultArchiver>
  </publishers>
  <buildWrappers>
    <hudson.plugins.build__timeout.BuildTimeoutWrapper>
      <strategy class="hudson.plugins.build_timeout.impl.AbsoluteTimeOutStrategy">
        <timeoutMinutes>15</timeoutMinutes>
      </strategy>
      <operationList>
        <hudson.plugins.build__timeout.operations.AbortOperation/>
      </operationList>
    </hudson.plugins.build__timeout.BuildTimeoutWrapper>
  </buildWrappers>
</project>